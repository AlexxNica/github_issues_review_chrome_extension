{
  "manifest_version": 2
, "name": "Github pull request review"
, "version": "0.1.0"
, "homepage_url": "http://www.teambox.com"
, "description": "https://github.com/micho/github_issues_review_chrome_extension"
, "permissions": [
    "https://github.com/",
    "cookies",
    "storage",
    "unlimitedStorage",
    "background"]
, "content_scripts": [
    {
      "matches": ["https://github.com/*"]
    , "js": ["lib/jquery-1.8.2.js","lib/underscore.js","lib/underscore_mixins.js","pull.js","issues.js"]
    }
  ]
}
